@model string

<!--========================== Participants list and Actions panel ==============================================-->
<ul id="actions-panel" class="sidenav sidenav-fixed animate__animated animate__fadeIn">
    <li><a class="subheader"><i class="material-icons grey-text">people</i>Participants</a></li>

    <li class="search-participant">
        <div class="input-field">
            <input id="participant-search" placeholder="Search" type="text" spellcheck="false" autocomplete="off">
        </div>
    </li>

    <li>
        <div class="participant-list">
            <ul>
                <li class="participant"><a class="blue-text">Fahad Khan</a></li>
                <li class="participant"><a class="blue-text">Mohammad Yousuf</a></li>
                <li class="participant"><a class="blue-text">Mirza Asad</a></li>
                <li class="participant"><a class="blue-text">Bilal Ahmed</a></li>
                <li class="participant"><a class="blue-text">Jamal Ahmed</a></li>
            </ul>
        </div>
    </li>

    <li><div class="divider"></div></li>

    <li><a class="subheader">Actions</a></li>

    <li>
        <div class="actions-list">
            <ul>
                <li><a class="waves-effect"><i class="material-icons">share</i>Share</a></li>
                <li><a class="waves-effect"><i class="material-icons">chat</i>Chat</a></li>
                <li><a class="waves-effect"><i class="material-icons">file_download</i>Download</a></li>
            </ul>
        </div>
    </li>
</ul>
